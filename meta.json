{
    "module_id": "viam:motion-detector",
    "visibility": "public",
    "url": "https://github.com/viam-labs/motion-detector/",
    "description": "Module of the Viam vision service that detects motion via camera.",
    "models": [
      {
        "api": "rdk:service:vision",
        "model": "viam:vision:motion-detector",
        "markdown_link": "README.md#example-attributes",
        "short_description": "A vision service that finds bounding boxes around motion"
      }
    ],
    "build": {
      "setup": "make setup",
      "build": "make dist/archive.tar.gz",
      "path": "dist/archive.tar.gz",
      "arch": ["linux/amd64", "linux/arm64"]
  },
  "entrypoint": "dist/main"
}
